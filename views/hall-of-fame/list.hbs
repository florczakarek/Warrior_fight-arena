<h2>Hall of fame</h2>
<table>
    <thead>
    <tr>
        <th>#</th>
        <th>Count of wins</th>
        <th>Name</th>
    </tr>
    </thead>
    <tbody>
    {{#each topWarriors}}
        <tr>
            <th>
                {{this.place}}
            </th>
            <td>
                {{this.warrior.wins}}
            </td>
            <td>{{this.warrior.name}}</td>
        </tr>
    {{/each}}
    </tbody>
</table>